<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
    
     <bean class="in.vs.beans.Student" id="stdId">
     <constructor-arg value="Vishal Soni"/>
     
     <constructor-arg >
     <list>
     <value>9193572517</value>
     <value>9557523909</value>
     </list>
     </constructor-arg>
     
     <constructor-arg>
     <set>
     <ref bean="add1"/>
     <ref bean="add2"/>
     </set>
     </constructor-arg>
     
      <constructor-arg>
     <map>
     <entry key="java" value="2 month"/>
     <entry key="Spring" value="2 month"/>
     </map>
     </constructor-arg> 
     </bean>
     
     
     <bean class="in.vs.beans.Address" id="add1">
       <constructor-arg value="22/7"/>
     <constructor-arg value="Agra"/>
     <constructor-arg value="282007"/>
     </bean>
     <bean class="in.vs.beans.Address" id="add2">
       <constructor-arg value="22/7"/>
     <constructor-arg value="Mathura"/>
     <constructor-arg value="18181"/>
     </bean>
</beans>