package in.vs.resources;

import org.springframework.stereotype.Component;

import in.vs.beans.Student;

@Component
public class SpringConfigFile {
	
	@Bean
	public Student stdId()
	{
		Student std=new Student();
		std.setName("Kabir");
		std.setRollNo(19);
		std.setEmail("kabir@gmail.com");
		return std;
	}

}
